{
	"name": "automattic/wp-super-cache",
	"description": "A very fast caching engine for WordPress that produces static html files.",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wp-super-cache",
		"wp-cache",
		"cache"
	],
        "support": {
                "issues": "https://github.com/Automattic/wp-super-cache/issues",
                "forum": "https://wordpress.org/support/plugin/wp-super-cache",
                "wiki": "https://github.com/Automattic/wp-super-cache/wiki",
                "source": "https://github.com/Automattic/wp-super-cache"
        },
	"require": {
		"php": ">=5.2"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "*",
		"wp-coding-standards/wpcs": "^0.14",
		"phpunit/phpunit": "6.*",
		"wimg/php-compatibility": "^8.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
	},
	"autoload-dev": {
	},
	"scripts": {
		"test": [
			"phpunit"
		],
		"phpcs": [
			"phpcs -s -p"
		],
	}
}
