{
	"name": "Automattic/wp-super-cache",
	"description": "A very fast caching engine for WordPress that produces static html files.",
	"type": "wordpress-plugin",
	"require": {
		"php": "^5.3.2||^7.0",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpcompatibility/phpcompatibility-wp": "^2.0.0"
		"WordPress-Coding-Standards/WordPress-Coding-Standards": "^1.2.*"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
	},
	"autoload-dev": {
	},
	"scripts": {
		"config-cs": [
			"\"vendor/bin/phpcs\" --config-set installed_paths ../../../vendor/wp-coding-standards/wpcs,../../../vendor/phpcompatibility/php-compatibility,../../../vendor/phpcompatibility/phpcompatibility-paragonie,../../../vendor/phpcompatibility/phpcompatibility-wp"
		],
		"check-cs": [
			"@php ./vendor/squizlabs/php_codesniffer/scripts/phpcs"
		],
		"check-cs-errors": [
			"@php ./vendor/squizlabs/php_codesniffer/scripts/phpcs --error-severity=1 --warning-severity=6"
		],
		"fix-cs": [
			"@php ./vendor/squizlabs/php_codesniffer/scripts/phpcbf"
		]
	}
}
